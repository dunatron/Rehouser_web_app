{
  "name": "rehouser-client",
  "version": "1.0.0",
  "engines": {
    "node": "12.x.x"
  },
  "description": "a tenancy solution via software",
  "main": "index.js",
  "author": "The Heaths",
  "license": "MIT",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start -p $PORT",
    "test": "NODE_ENV=test jest --watch",
    "heroku-postbuild": "next build",
    "now-build": "next build",
    "format": "prettier \"**/*.{js,ts,tsx,css,graphql}\" --write",
    "stripe:listen": "stripe listen --forward-to http://localhost:4444/stripe/webhook",
    "stripe:trigger:created": "stripe trigger payment_intent.created",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "lint": "eslint ./components/** ./pages/** -c .eslintrc.js --ext js,jsx",
    "lint-fix": "eslint ./components/** ./pages/** -c .eslintrc.js --fix --ext js,jsx",
    "gen-prop-types": "npx react-proptypes-generate project components/"
  },
  "dependencies": {
    "@apollo/client": "^3.3.8",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@material-ui/core": "4.11.2",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.42",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "apollo-upload-client": "^14.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.12.0",
    "cloudinary-react": "^1.6.8",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "downshift": "^6.0.12",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
    "lodash.debounce": "^4.0.8",
    "material-ui-image": "^3.3.1",
    "next": "^10.0.5",
    "next-transpile-modules": "^6.3.0",
    "next-with-apollo": "^5.1.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-card-flip": "^1.1.0",
    "react-dom": "^17.0.1",
    "react-toastify": "^7.0.3",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.2.1",
    "uuidv4": "^6.2.6",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "^0.13.7",
    "casual": "^1.5.19",
    "cross-env": "^7.0.2",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^7.8.1",
    "eslint-plugin-react": "^7.20.6",
    "graphql-tools": "^4.0.0",
    "jest": "^23.6.0",
    "jest-transform-graphql": "^2.1.0",
    "logrocket": "^1.0.10",
    "next-sitemap": "^1.4.5",
    "react-proptypes-generate": "^1.6.1",
    "typescript": "^3.6.3"
  }
}
